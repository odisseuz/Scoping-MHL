	note = {ERIC Number: EJ1337365},
	keywords = {Adolescents, Mental Health, State Legislation, School Health Services, Children, Disadvantaged Schools, Access to Health Care, Autism, Health Programs, Pervasive Developmental Disorders, Telecommunications},
