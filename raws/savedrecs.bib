@article{ WOS:001003912900009,
Author = {Drew, Breanna J. and Sutcliffe, Jordan T. and Bruner, Mark W. and
   McLaren, Colin D. and Swann, Christian and Schweickle, Matthew J. and
   Vella, Stewart A.},
Title = {Mental Health Literacy Workshop for Youth Sport Coaches: A Mixed-Methods
   Pilot Study},
Journal = {INTERNATIONAL SPORT COACHING JOURNAL},
Year = {2023},
Volume = {10},
Number = {1},
Pages = {91-101},
Month = {JAN},
Abstract = {Among other responsibilities, youth sport coaches are positioned to
   monitor and address the mental health needs of their athletes. Despite
   this, there are limited interventions aimed at improving coaches' mental
   health literacy. Using a mixed-methods design, the aim of this pilot
   study was to evaluate the feasibility, acceptability, and mental health
   literacy outcomes associated with a brief (75 min) workshop for youth
   sport coaches. Fourteen coaches (13 males, one female) completed pre-
   and postworkshop surveys measuring indices of mental health literacy,
   and 10 of these same participants engaged in a semistructured interview
   1-month later. Overall, coaches who participated in the pilot workshop
   reported significant improvements in depression literacy, intentions to
   seek self-help for oneself and their athletes, and knowledge and
   confidence to provide help. In addition, coaches reported positive
   impressions of the workshop during the follow-up interviews and provided
   concrete examples of program content application. An important
   suggestion made by coaches was the need to align the workshop content to
   governing policy. Taken together, this pilot mental health literacy
   workshop for youth sport coaches shows strong promise and is ready for
   large-scale dissemination.},
