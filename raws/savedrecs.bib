@article{ WOS:001503444300001,
Author = {Gong, Chenxin and Gu, Yingqi and Zhao, Tongshu and Hu, Hengyu},
Title = {Temporal dynamics of mindfulness meditation on adolescent attention: A
   longitudinal study},
Journal = {CURRENT PSYCHOLOGY},
Year = {2025},
Volume = {44},
Number = {12},
Pages = {12258-12273},
Month = {JUN},
Abstract = {Mindfulness has been widely shown to enhance attention, yet its effects
   on different attentional functions and the required intervention
   duration remain inconsistent. Precise findings regarding attentional
   improvements can inform targeted interventions for specific ADHD
   subtypes. This study investigated the temporal progression of
   improvements in attentional functions during mindfulness interventions
   using a longitudinal design. A total of 116 participants were randomly
   assigned to three groups: mindfulness meditation (n = 39), music
   relaxation (n = 39), and mental health education (n = 38). Ultimately,
   90 participants (30 per group) completed the study. Baseline data were
   collected through demographic questionnaires, the Toronto Mindfulness
   Scale (TMS), a psychological distress questionnaire, and the Attention
   Network Test (ANT). During the five-week intervention, participants
   practiced their assigned activities for 10 min daily (excluding
   weekends) and completed state mindfulness assessments and ANT testing at
   weeks 1, 3, and 5. After controlling for baseline measures, no
   significant linear increase in state mindfulness was found across the
   groups. However, the mindfulness group demonstrated significant and
   sustained improvements in executive control from the first week onward,
   outperforming the control groups. In contrast, no significant changes
   were observed in alerting or orienting functions throughout the
   intervention. These findings suggest that mindfulness primarily enhances
   executive control, while its effects on alerting and orienting functions
   remain unclear and warrant further investigation.},
