@article{ WOS:000456302600005,
Author = {Eschenbeck, Heike and Lehner, Laya and Hofmann, Hanna and Bauer,
   Stephanie and Becker, Katja and Diestelkamp, Silke and Kaess, Michael
   and Moessner, Markus and Rummel-Kluge, Christine and Salize,
   Hans-Joachim and Thomasius, Rainer and Bertsch, Katja and Bilic, Sally
