@article{ WOS:000405992100358,
Author = {Liu, Xiaohua},
Title = {Research on mental health education in higher vocational colleges under
   the intervention of the rational emotional model},
Journal = {AGRO FOOD INDUSTRY HI-TECH},
Year = {2017},
Volume = {28},
Number = {1},
Pages = {1637-1641},
Month = {JAN-FEB},
Abstract = {Higher vocational education has a key role in the cultivation of new
   talents and the development of the country through science and
   education. Psychological education differs from other fields of higher
   learning because different methods and models of mental health education
   are employed according to their characteristics. The psychological
   health education in higher vocational colleges under the intervention of
   the rational emotional model was studied in this paper. After
   introducing the present situation of psychological education in higher
   vocational colleges at home and abroad as well as the related theories
   and technologies, the mental state of vocational students and the
   educational characteristics of colleges and universities were used to
   explore the psychological education mode of these institutions. Several
   methods for emotional guidance were devised, and a series of
   interventions for mental health education in higher vocational colleges
   was proposed. The mental health status of vocational students before and
   after the implementation of the rational emotional intervention was
   tested and analyzed. The experimental results reveal that the rational
   emotional model positively affects mental health education in higher
   vocational colleges and that the rational emotional intervention
   significantly improves the psychological status of students.},
Publisher = {TEKNOSCIENZE PUBL},
Address = {VIALE BRIANZA 22, 20127 MILANO, ITALY},
