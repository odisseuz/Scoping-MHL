@article{ WOS:001610845100001,
Author = {Williams, Laura and Bartik, Warren and Cosh, Suzie},
Title = {Interventions to facilitate mental health help-seeking for young rural
   Australians through stigma reduction: a three-arm randomised trial},
Journal = {ADVANCES IN MENTAL HEALTH},
Year = {2025},
Month = {2025 NOV 8},
Abstract = {ObjectiveThis study aimed to assess the effectiveness of two
   intervention strategies, Internet-Based Cognitive Behavioural Therapy
   (ICBT) and Cognitive Bias Modification for Help-Seeking (CBM-HS), in
   modifying young, rural Australians' mental health help-seeking stigma to
   increase intentions to access mental health care within
   Australia.MethodA total of 42 young people (aged between 15 and 24
   years) participated in a three-armed randomised trial, with participants
   randomly allocated into one of the two interventions or the control
   condition. Data were collected pre-, post-, and at six weeks follow-up
   via self-report questionnaires. Main outcome measures: mental health
   help-seeking self-stigma, perceived public stigma, attitudes, and
   intentions to seek mental health help.ResultsThe results provided some
   support for the CBM-HS intervention as effective in decreasing
   self-stigma across the three time points compared to the other
   conditions. No significant effects were observed in the ICBT
   intervention. A significant increase in mean mental health literacy
   scores was also found between time points across all
   conditions.ConclusionsThese findings suggest that online anti-stigma
   interventions could feasibly be used to reduce help-seeking stigma among
   young rural Australians, which may help to increase the proportions of
   individuals seeking help for their mental health concerns.},
