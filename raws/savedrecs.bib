@article{ WOS:000404221700006,
Author = {Clement, Marie-Eve and Gagne, Marie-Helene and Brunson, Liesette},
Title = {Analysis of the sources of professional support for parenting for
   mothers of 0-8 years old children},
Journal = {CANADIAN JOURNAL OF BEHAVIOURAL SCIENCE-REVUE CANADIENNE DES SCIENCES DU
   COMPORTEMENT},
Year = {2017},
Volume = {49},
Number = {2},
Pages = {112-121},
Month = {APR},
Abstract = {Few studies have dealt with parents search for professional help, or the
   factors related to consultation by sectors of intervention (health,
   mental health, education). In response to that gap, this study seeks to
   document which professionals are consulted during a year in relation
   with child education or behaviour and what individual, family and social
   factors are related to such support. The data result from a telephone
   survey of a representative, weighted sample of 2326 Quebec mothers of
   children between the ages of 6 months and 8 years of age. About one
   mother in three stated that she had consulted at least one professional
   about parenting during the year. Logistic regression analysis shows that
   the mothers who consult most frequently do so concerning an older boy
   who is experiencing emotional or hyperactive problems. Compared to
   mothers who do not consult professionals, the mothers who do consult
   display various psychosocial issues, including single parenting, a weak
   sense of parental skills, and increased parental stress; they also
   report more frequent recourse to coercive conduct with their child. Path
   analyses, for their part, reveal that the mother's characteristics are
   different depending on whether they have consulted a health
   professional, a mental health or education professional. The results
   raise many issues about normalization of the search for help by parents,
   about the training of professionals and their ability to respond to the
   needs of the mothers who consult them.},
