@article{ WOS:000280260200005,
Author = {Shandley, Kerrie and Austin, David and Klein, Britt and Kyrios, Michael},
Title = {An evaluation of `Reach Out Central': an online gaming program for
   supporting the mental health of young people},
Journal = {HEALTH EDUCATION RESEARCH},
Year = {2010},
Volume = {25},
Number = {4},
Pages = {563-574},
Month = {AUG},
Abstract = {The objective of this study was to conduct an evaluation of Reach Out
   Central (ROC), an online gaming program designed to support the mental
   health of people aged 16-25. The evaluation sought to determine the
   benefit of playing ROC on alcohol use, use of coping strategies,
   psychological distress, resilience and satisfaction with life. Changes
   in mental health literacy, mental health stigma and willingness to seek
   help and program satisfaction were also investigated. A single group (N
   = 266) quasi-experimental repeated measures (pre-, post-program, 2-month
   follow-up) design was employed. The results demonstrated positive
   improvements across all outcome measures for females; however, a
   non-significant worsening effect was observed for males on seeking
   support, avoidance and resilience. Improvements for both genders were
   observed on mental health literacy and help-seeking. However, literacy
   levels and help-seeking were significantly higher, and stigma
   significantly lower for females. Program satisfaction ratings were high
