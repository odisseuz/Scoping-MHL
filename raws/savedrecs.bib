@article{ WOS:000402166800004,
Author = {Yamauchi, Takashi and Takeshima, Tadashi and Hirokawa, Seiko and Oba,
   Yoshitaka and Koh, Eugen},
Title = {An Educational Program for Nursing and Social Work Students Using
   Artwork Created by People with Mental Health Problems},
Journal = {INTERNATIONAL JOURNAL OF MENTAL HEALTH AND ADDICTION},
Year = {2017},
Volume = {15},
Number = {3},
Pages = {503-513},
Month = {JUN},
Abstract = {No empirical study has examined the effect of artwork created by people
   with mental health problems on the attitudes of nursing and social work
   students, separately in those who had had and had not had past close
   contact with those individuals. We preliminarily investigated whether an
   educational program using artwork created by people with mental health
   problems could promote positive attitudes among nursing and social work
   students in Japan, regardless of past close contact experiences. A
   pre-/post-comparison design survey was conducted at two universities and
   two colleges in Japan. A total of 307 nursing and social work students
   rated their attitudes on a semantic differential attitude scale at both
   pre-program (T1) and post-program (T2) stages. Wilcoxon's signed rank
   tests revealed that participants showed more neutral to positive
   attitudes toward people with mental health problems at T2 than at T1,
   regardless of past close contact with individuals with such problems.
   The findings imply that our educational program provided participants
   with new perspectives of people with mental health problems, which were
   different from those provided by basic mental health education or
   obtained through close contact. Nursing and social work education that
   utilizes artwork created by people with mental health problems may
   cultivate sympathetic and positive attitudes toward those individuals.},
