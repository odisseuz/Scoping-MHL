@article{ WOS:001310790400001,
Author = {Thomas, Cassidie S. and Nielsen, Tiffany K. and Best, Nakia C.},
Title = {A Rapid Review of Mental Health Training Programs for School Nurses},
Journal = {JOURNAL OF SCHOOL NURSING},
