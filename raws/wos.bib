@article{ WOS:001183382800002,
Author = {Mai, Katherine and Dawson, Anne E. and Gu, Lidan and Lamotte, Julia E.
   and Molitor, Stephen and Zimmerman, Cortney T.},
Title = {Common mental health conditions and considerations in pediatric chronic
   kidney disease},
Journal = {PEDIATRIC NEPHROLOGY},
