@article{ WOS:000591881600001,
Author = {Brand, Gabrielle and Sheers, Carli and Wise, Steve and Seubert, Liza and
   Clifford, Rhonda and Griffiths, Pamela and Etherton-Beer, Christopher},
Title = {A research approach for co-designing education with healthcare consumers},
Journal = {MEDICAL EDUCATION},
