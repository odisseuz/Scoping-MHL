@article{ WOS:000537660300003,
Author = {Wang, Kai and Zhou, Yue-e and Xu, Jia-xin and Yang, Guang},
Title = {RETRACTED: Reviewing big data based mental health education process for
   promoting education system (Retracted Article)},
Journal = {CURRENT PSYCHOLOGY},
Year = {2022},
Volume = {41},
Number = {5},
Pages = {3297},
Month = {MAY},
Abstract = {Most of the people affected by mental disorder especially adolescent and
   children which need to be taken attention in earlier stage. There is
   lots of mental education model need to be created for creating positive
   thoughts to the mental health problem people. So, in this paper ultimate
   goal to develop the effective big data based mental health education
   promoting schools to support their positive thoughts. During this
   analysis process, different data such as health, social impacts,
   education, literature, humanities and art related data is collected with
   the help of the map reducing big data technique. From the gathered
   information mental health schooling process is determined for promoting
   their activities and detects the mental problem effectively. This mental
   health education model increase the people learning process also reduces
