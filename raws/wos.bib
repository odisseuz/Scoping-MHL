@article{ WOS:000596767700001,
Author = {Chew, Chu Shan Elaine and Kelly, Siobhan and Tay, E. Eric and Baeg,
   Amerie and Khaider, Khairunisa Binte and Oh, Jean Yin and Rajasegaran,
   Kumudhini and Saffari, Seyed Ehsan and Davis, Courtney},
Title = {Implementation of family-based treatment for Asian adolescents with
   anorexia nervosa: A consecutive cohort examination of outcomes},
Journal = {INTERNATIONAL JOURNAL OF EATING DISORDERS},
Year = {2021},
Volume = {54},
Number = {1, SI},
Pages = {107-116},
Month = {JAN},
Abstract = {Objective This study describes the implementation of family-based
   treatment (FBT) in an eating disorder program in Asia as well as
   clinical outcomes of Asian adolescents with anorexia nervosa (AN)
   treated with FBT.
   Method This retrospective consecutive cohort study of 147 Asian
   adolescents with AN was compared between those in FBT (n = 65) versus
   treatment as usual (TAU) (n = 82). Variables associated with weight
   restoration were analyzed between groups.
   Results Participants' mean age was 14.2 (SD = 1.5) years and 93\% were
   female. Mean presenting \%mBMI was 74.0 (SD = 7.8) and average illness
   duration was 7.7 (SD = 6.1) months. The two groups' baseline
   characteristics were not significantly different. Weight restoration
   rates in the FBT group were significantly higher than the TAU group at
   6-, 12-, and 24-month time points. A linear mixed model showed the mean
   \%mBMI was significantly higher at 0, 6, 12, and 24 months in the FBT
   group. The median time to weight restoration for patients on FBT was
   shorter (FBT: 7.0 months, TAU: 19.0 months; 95\% CI {[}14.5, 23.5]
   chi(2) = 15.84, p < .001). Within the FBT group, those that completed >=
   9 FBT sessions had significantly higher rates of weight restoration at
   12 months. Across all participants, those with a lower starting \%mBMI
   were less likely to achieve weight restoration by 12 months.
   Conclusion FBT can be effectively implemented in a multidisciplinary
   eating disorder program managing Asian adolescents with AN with improved
   rates of weight restoration. Further research is needed to understand
   the predictors and moderators of remission using FBT in Asian
   adolescents with AN.},
