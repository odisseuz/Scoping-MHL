@article{ WOS:001479728200001,
Author = {Srinivasan, Shiva Prakash and Behera, Aloka Datta and Arumugam,
   Chiranjeevi and Durairaj, Jothilakshmi and Panda, Protush and Rangeela,
   E. and Nayak, Smruti Smitha and Raghavan, Vijaya and Ramachandran,
   Padmavati},
Title = {The effect of a low intensity intervention on the wellbeing of children
   in the juvenile justice system in India: Results from a pilot study},
Journal = {ASIAN JOURNAL OF PSYCHIATRY},
Year = {2025},
Volume = {108},
Month = {JUN},
Abstract = {Objective Children in the Juvenile Justice System (JJS) face multiple
   adversities that may predispose them to developing mental health (MH)
   problems. Interventions that enhance the MH and well-being of these
   children while providing access to appropriate MH care are needed. This
   study examines the effect of a multipronged, low-intensity intervention
   on adolescents' well-being, stress levels, MH knowledge, and stigma
   perceptions in Childcare Institutions (CCIs) across Odisha and Tamil
   Nadu, India. Methods Implemented in 7 CCIs, the intervention aimed to
   enhance the MH Literacy (MHL) and well-being of children in the CCIs
   through the Youth-Friendly Spaces (YFS) using experiential methods. It
   also sought to improve the MHL of the staff and administration within
   the JJS and provide access to MH care by linking CCIs to available local
   resources. Information using validated scales was obtained at baseline
   and six months. Results Only 180 of the 310 children for whom baseline
   data was available remained in the CCI at six months. A statistically
   significant improvement in well-being (34.5 +13.7-55.9 +12.2, p <
   0.001), stress (18.5 +6.8-17 +6.1, p = 0.044) and stigma (32 +11.9-29.4
   +11.4, p = 0.023) scores were observed. Adjusted linear regression
   analysis showed significant differences across genders, sites, and child
   types. Discussion This intervention, comprising YFS creation, MHL
   enhancement, and referral system development, is significantly
   associated with improved well-being of children in CCIs. The results
   underscore the need for tailored interventions based on gender,
   location, and child type. The study highlights the potential scalability
   of such programs in resource-constrained settings for vulnerable
   children.},
