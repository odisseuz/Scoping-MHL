@article{ WOS:000874007500001,
Author = {Yang, Guangyan},
Title = {RETRACTED: Research on Mental Health Monitoring Scheme of Migrant
   Children Based on Convolutional Neural Network Based on Deep Learning
   (Retracted Article)},
